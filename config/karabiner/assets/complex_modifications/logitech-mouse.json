{
  "title": "Logitech Mouse Mappings",
  "rules": [
    {
      "description": "Right-cmd-open-bracket to Right-cmd-left-arrow",
      "manipulators": [
        {
          "from": {
            "key_code": "open_bracket",
            "modifiers": {
              "mandatory": [
                "right_command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Right-cmd-close-bracket to Right-cmd-right-arrow",
      "manipulators": [
        {
          "from": {
            "key_code": "close_bracket",
            "modifiers": {
              "mandatory": [
                "right_command"
              ],
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "right_arrow",
              "modifiers": [
                "right_command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    },
    {
      "description": "Mouse-button4 to Left-cmd-left-arrow",
      "manipulators": [
        {
          "from": {
            "pointing_button": "button4",
            "modifiers": {
              "optional": [
                "any"
              ]
            }
          },
          "to": [
            {
              "key_code": "left_arrow",
              "modifiers": [
                "command"
              ]
            }
          ],
          "type": "basic"
        }
      ]
    }
  ]
}
